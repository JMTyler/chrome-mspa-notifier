{
	"name": "NEW Homestuck Notifier",
	"description": "Notifies you of updates to Homestuck.com (such as The Epilogues) & keeps track of your progress.",
	"version": "2.1.0",
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"browser_action": {
		"default_icon": {
			"16": "icons/16.png",
			"32": "icons/32.png"
		}
	},
	"background": {
		"scripts": [
			"settings.js",
			"memory.js",
			"version.js",
			"background.js"
		],
		"persistent": true
	},
	"options_page": "options.html",
	"permissions": [
		"contextMenus",
		"gcm",
		"notifications",
		"storage",
		"tabs",
		"https://www.homestuck.com/*"
	],
	"web_accessible_resources": ["icons/48.png", "icons/128.png"],
	"minimum_chrome_version": "62",
	"manifest_version": 2
}
