{
	"name": "MSPA Notifier",
	"description": "MSPA Notifier keeps track of your spot on MSPaintAdventures.com and notifies you of new updates.",
	"version": "1.4.2",
	"icons": {
		"16": "mspa_face-16.png",
		"48": "48.png"
	},
	"browser_action": {
		"default_icon": "mspa_face.gif"
	},
	"background": {
		"scripts": ["settings.js", "memory.js", "version.js", "background.js"],
		"persistent": true
	},
	"options_page": "options.html",
	"permissions": [
		"tabs",
		"notifications",
		"contextMenus",
		"*://*.mspaintadventures.com/*"
	],
	"web_accessible_resources": ["48.png"],
	"manifest_version": 2
}
