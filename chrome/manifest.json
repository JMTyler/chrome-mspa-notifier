{
	"name": "Homestuck Notifier",
	"description": "Homestuck Notifier keeps track of your spot on Homestuck.com and notifies you of new updates.",
	"version": "1.4.2",
	"icons": {
		"16": "mspa_face-16.png",
		"48": "48.png"
	},
	"browser_action": {
		"default_icon": "mspa_face.gif"
	},
	"background": {
		"scripts": [
			"settings.js",
			"memory.js",
			"version.js",
			"background.js"
		],
		"persistent": true
	},
	"options_page": "options.html",
	"permissions": [
		"tabs",
		"notifications",
		"contextMenus",
		"*://*.mspaintadventures.com/*",
		"https://www.homestuck.com/*",
		"gcm"
	],
	"web_accessible_resources": ["48.png"],
	"manifest_version": 2
}
